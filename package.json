{
  "name": "weblogjs",
  "version": "0.0.4",
  "description": "a library for weblogs",
  "main": "./app",
  "scripts": {
    "init": "WEBLOG_ENV={^mode^:^init^} node ./runner/index",
    "development": "WEBLOG_ENV={^mode^:^development^,^web_port^:3001,^webpack_server_host^:^localhost^,^webpack_server_port^:8001} node ./runner/index",
    "webpack-dev-server": "WEBLOG_ENV={^mode^:^webpack-dev-server^,^web_port^:3001,^webpack_server_port^:8001} ./node_modules/.bin/webpack-dev-server --inline --hot --port=8001 --host=localhost",
    "production": "WEBLOG_ENV={^mode^:^production^,^web_port^:3001} node ./runner/index",
    "webpack": "WEBLOG_ENV={^mode^:^production^,^web_port^:3001} ./node_modules/.bin/webpack",
    "test": "WEBLOG_ENV={^mode^:^test^,^web_port^:3002} ./node_modules/.bin/mocha $(find test -name '*.test.js') --compilers js:babel-core/register"
  },
  "author": {
    "name": "Ippei Tanaka",
    "email": "tanaka.ippei.jp@gmail.com",
    "url": "http://www.ipe.club/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ippei-tanaka/weblogjs.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.14.2",
    "chai": "^0.5.3",
    "classnames": "^2.2.1",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.1",
    "css-loader": "^0.23.1",
    "deepcopy": "^0.6.1",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "hat": "0.0.3",
    "history": "^1.13.1",
    "immutable": "^3.7.6",
    "jquery": "^2.1.4",
    "keymirror": "^0.1.1",
    "mocha": "^2.3.4",
    "moment": "^2.10.6",
    "mongodb": "^2.1.14",
    "mongoose": "^4.3.6",
    "mongoose-unique-validator": "^0.6.2",
    "mongoose-validator": "^1.2.4",
    "morgan": "^1.6.1",
    "node-sass": "^3.4.2",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "pluralize": "^1.2.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.1.2",
    "react-router": "^1.0.2",
    "react-router-redux": "^2.1.0",
    "redux-devtools": "^3.0.2",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "request": "^2.67.0",
    "sass-loader": "^3.1.2",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.0",
