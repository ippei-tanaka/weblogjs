{
  "name": "weblogjs",
  "version": "0.0.7-alpha.1",
  "description": "a library for weblogs",
  "main": "./app",
  "scripts": {
    "build": "npm run build:clean && npm run build:server && npm run build:client && npm run build:copy-client",
    "build:clean": "./node_modules/.bin/rimraf lib",
    "build:server": " ./node_modules/.bin/babel src --out-dir lib --ignore src/client",
    "build:client": "./node_modules/.bin/babel-node ./src/runner webpack",
    "build:copy-client": "./node_modules/.bin/mkdirp lib/client/static && ./node_modules/.bin/ncp src/client/static lib/client/static",
    "dev:webpack-dev-server": "./node_modules/.bin/babel-node ./src/runner webpack-dev-server",
    "dev:development": "./node_modules/.bin/babel-node ./src/runner development",
    "dev:webpack": "./node_modules/.bin/babel-node ./src/runner webpack",
    "dev:production": "./node_modules/.bin/babel-node ./src/runner production",
    "dev:init": "./node_modules/.bin/babel-node ./src/runner init",
    "dev:test": "./node_modules/.bin/babel-node ./src/runner test",
    "production": "node ./lib/runner production",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Ippei Tanaka",
    "email": "tanaka.ippei.jp@gmail.com",
    "url": "http://www.ipe.club/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ippei-tanaka/weblogjs.git"
  },
  "bin": {
    "weblogjs": "./lib/runner/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "bcrypt": "^0.8.6",
    "body-parser": "^1.15.1",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.3",
    "deepcopy": "^0.6.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "keymirror": "^0.1.1",
    "moment": "^2.13.0",
    "mongodb": "^2.1.21",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "pluralize": "^1.2.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "superagent": "^1.8.3",
    "validator": "^5.3.0"
  },
  "keywords": [
    "blog",
    "weblog",
    "weblogjs"
  ],
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "ncp": "^2.0.0",
    "node-sass": "^3.7.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-sources": "^0.1.2"
  }
}
