{
  "name": "weblogjs",
  "version": "0.6.1",
  "description": "a library for weblogs",
  "main": "./lib/src",
  "scripts": {
    "build": "npm-run-all clean --parallel \"build:*\"",
    "build:source": "babel src --out-dir lib/src",
    "build:scripts": "babel scripts --out-dir lib/scripts",
    "build:test": "babel test --out-dir lib/test",
    "clean": "rimraf lib",
    "development": "babel-node ./scripts/development",
    "init": "babel-node ./scripts/init",
    "test": "npm run build && babel-node ./lib/scripts/test",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Ippei Tanaka",
    "email": "tanaka.ippei.jp@gmail.com",
    "url": "http://www.ipe.club/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ippei-tanaka/weblogjs.git"
  },
  "bin": {
    "weblogjs": "./lib/scripts/cli.js"
  },
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-runtime": "^6.9.2",
    "bcrypt": "^0.8.6",
    "body-parser": "^1.15.1",
    "brace": "^0.8.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.3",
    "deepcopy": "^0.6.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "keymirror": "^0.1.1",
    "moment": "^2.13.0",
    "mongodb": "^2.1.21",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "pluralize": "^1.2.1",
    "react": "^15.1.0",
    "react-ace": "^3.4.1",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
