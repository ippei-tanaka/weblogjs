{
  "name": "weblogjs",
  "version": "0.7.10",
  "description": "a library for weblogs",
  "main": "./lib/app.js",
  "scripts": {
    "compile": "NODE_ENV=production npm-run-all compile:*",
    "compile:clear": "rimraf lib",
    "compile:build": "babel ./src --out-dir lib --copy-files --ignore bundle",
    "compile:copy-admin": "mkdirp ./lib/admin-app/static/bundle && ncp ./src/admin-app/static/bundle ./lib/admin-app/static/bundle",
    "compile:copy-public": "mkdirp ./lib/public-app/static/bundle && ncp ./src/public-app/static/bundle ./lib/public-app/static/bundle",
    "bundle-brs": "NODE_ENV=production npm-run-all clear-brs --parallel \"bundle-brs:*\"",
    "bundle-brs:admin-js": "webpack --config ./src/admin-app/webpack.config.babel.js",
    "bundle-brs:admin-css": "node-sass ./src/admin-app/style/main.scss -o ./src/admin-app/static/bundle",
    "bundle-brs:public-css": "node-sass ./src/public-app/style/themes -o ./src/public-app/static/bundle/themes",
    "watch-brs": "npm-run-all clear-brs --parallel \"watch-brs:*\"",
    "watch-brs:admin-js": "npm run bundle-brs:admin-js -- -w",
    "watch-brs:admin-css": "npm run bundle-brs:admin-css && npm run bundle-brs:admin-css -- -w",
    "watch-brs:public-css": "npm run bundle-brs:public-css && npm run bundle-brs:public-css -- -w",
    "clear-brs": "rimraf ./src/admin-app/static/bundle && rimraf ./src/public-app/static/bundle",
    "build": "npm run bundle-brs && npm run compile",
    "start-dev": "babel-node ./scripts/start-dev",
    "start": "NODE_ENV=production npm run build && node ./scripts/start",
    "test": "NODE_ENV=production npm run build && mocha $(find test -name '*.test.js') --compilers js:babel-core/register",
    "test-src": "mocha $(find test -name '*.test.js') --compilers js:babel-core/register",
    "prepublish": "NODE_ENV=production npm run build"
  },
  "bin": {
    "weblogjs": "./scripts/start.js"
  },
  "author": {
    "name": "Ippei Tanaka",
    "email": "tanaka.ippei.jp@gmail.com",
    "url": "http://www.ipe.club/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ippei-tanaka/weblogjs.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-runtime": "^6.9.2",
    "bcrypt": "^0.8.6",
    "body-parser": "^1.15.1",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.3",
    "deepcopy": "^0.6.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "highlight.js": "^9.5.0",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "keymirror": "^0.1.1",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "moment": "^2.13.0",
    "mongodb": "^2.1.21",
    "morgan": "^1.7.0",
    "npm-run-all": "^2.3.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pluralize": "^1.2.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "showdown": "^1.4.2",
    "showdown-prettify": "^1.3.0",
    "simple-odm": "^0.4.0",
    "superagent": "^1.8.3",
    "url-loader": "^0.5.7",
    "validator": "^5.3.0"
  },
  "keywords": [
    "blog",
    "weblog",
    "weblogjs"
  ],
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "concurrently": "^2.2.0",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^3.7.0",
    "nodemon": "^1.10.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-sources": "^0.1.2"
  }
}
